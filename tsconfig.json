{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "paths": {
      "#veewme/*": ["./src/*"],
      "#veewme/gen/*": [
        "./build/gen/*"
      ],
      "#veewme/graphql/types": [
        "./build/gen/graphqlTypes.tsx"
      ]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "build",
    "dist",
    "node_modules"
  ],
  "include": [
    "./src/**/*"
  ]
}
