  },
  "scripts": {
    "build": "npm-run-all -l -s prebuild -p build:server build:web build:functions",
    "build:dev": "cross-env-shell NODE_ENV=development run-s build",
    "build:prod": "cross-env-shell NODE_ENV=production run-s build",
    "build:functions": "webpack --colors --config-name functions",
    "build:server": "webpack --colors --config-name server",
    "build:web": "webpack --colors --config-name web",
    "build:web:dev": "cross-env-shell NODE_ENV=development run-s build:web",
    "dev": "npm-run-all -l -s prebuild -p -r dev:server dev:web dev:functions",
    "dev:functions": "cross-env-shell NODE_ENV=development DEV_MODE=true TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling webpack --colors --config-name functions --watch",
    "dev:server": "cross-env-shell NODE_ENV=development DEV_MODE=true TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling webpack --colors --config-name server --watch",
    "dev:web": "cross-env-shell NODE_ENV=development DEV_MODE=true TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling webpack-dev-server --colors --config-name web",
    "docker:attach": "docker-compose exec dev /bin/bash",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up",
    "env:print": "node ./src/build/scripts/printEnv",
    "functions:start": "npx functions-framework --source src/functions --target=testFunction",
    "lint": "run-p lint:js lint:pkgjs",
    "lint:js": "tslint **.js src/**/**.js",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:pkgjs": "npmPkgJsonLint --configFile .npmpackagejsonlintrc.json package.json",
    "prebuild": "node ./src/build/scripts/prebuild",
    "prisma:delete": "prisma delete -f",
    "prisma:deleteAndCreate": "npm-run-all prisma:delete prisma:deploy prisma:seed",
    "prisma:deploy": "prisma deploy --no-seed -f",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma reset -f",
    "prisma:resetAndCreate": "npm-run-all prisma:reset prisma:deploy prisma:seed",
    "prisma:seed": "prisma seed",
    "start": "node dist/server.js",
    "todo": "git grep -e TODO -e FIXME -e XXX"
  },
